<?xml version="1.0" encoding="UTF-8"?>
<connector 	xmlns="http://java.sun.com/xml/ns/j2ee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd"
  version="1.5">
  <description>RV_JCA_Adapter</description>
  <display-name>RV_JCA_ResourceAdapter</display-name>
  <vendor-name>TIBCO</vendor-name>
  <eis-type>TIBCO Rendezvous</eis-type>
  <resourceadapter-version>1.0</resourceadapter-version>
  <license>
    <description>TIBCO Rendezvous Resource Adapter</description>
    <license-required>false</license-required>
  </license>
  <resourceadapter>
    <resourceadapter-class>com.tibco.rv.jca.ra.RVInboundResourceAdapter</resourceadapter-class>
    <inbound-resourceadapter>
		<messageadapter>
			<messagelistener>
				<messagelistener-type>com.tibco.rv.jca.RVListener</messagelistener-type>
				<activationspec>
					<activationspec-class>com.tibco.rv.jca.ra.RVInboundActivationSpec</activationspec-class>
					<required-config-property>
               	       <config-property-name>daemon</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>network</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>service</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>subject</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>mode</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>dqWeight</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>dqTasks</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>dqSchedulerWeight</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>dqSchedulerHeartBeat</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>dqSchedulerActivation</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftGroup</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftWeight</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftActive</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftActiveGoal</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftHeartBeat</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftPrepInterval</config-property-name>
               	    </required-config-property>
               	    <required-config-property>
               	       <config-property-name>ftActivationInterval</config-property-name>                    	       
					</required-config-property>
				</activationspec>
			</messagelistener>
		</messageadapter>
	</inbound-resourceadapter>
  </resourceadapter>
</connector>
